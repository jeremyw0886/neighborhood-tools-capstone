# Application
APP_NAME=NeighborhoodTools
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost

# Database (production only — local dev uses hardcoded MAMP config)
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=neighborhoodtools
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Session
SESSION_LIFETIME=120

# Google Geocoding API — required for registering with ZIP codes not already in the database
# Get a key: https://console.cloud.google.com/apis/credentials
# Enable: Geocoding API
GOOGLE_GEOCODING_API_KEY=
